{
  "compileOnSave": true,
  "compilerOptions": {
    "alwaysStrict": true,
    "emitDecoratorMetadata": true,
    "importsNotUsedAsValues": "error",
    "lib": ["esnext", "dom", "deno.ns"],
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "strict": true
  }
}
